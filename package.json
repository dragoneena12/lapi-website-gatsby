{
  "name": "lapi-website-gatsby",
  "private": true,
  "description": "Lapi-ruriiro's personal website built with Gatsby",
  "version": "0.1.0",
  "author": "lapi-ruriiro <tyoutugai.7@gmail.com>",
  "dependencies": {
    "@auth0/auth0-react": "^2.0.0",
    "date-fns": "^2.23.0",
    "dotenv": "^16.4.5",
    "gatsby": "^5.0.0",
    "gatsby-plugin-google-analytics": "^5.0.0",
    "gatsby-plugin-image": "^3.0.0",
    "gatsby-plugin-layout": "^4.0.0",
    "gatsby-plugin-manifest": "^5.0.0",
    "gatsby-plugin-remove-serviceworker": "^1.0.0",
    "gatsby-plugin-sass": "^6.0.0",
    "gatsby-plugin-sharp": "^5.0.0",
    "gatsby-plugin-use-query-params": "^1.0.1",
    "gatsby-source-filesystem": "^5.0.0",
    "gatsby-transformer-sharp": "^5.0.0",
    "graphql": "^16.8.1",
    "node-sass": "^9.0.0",
    "qrcode.react": "^3.0.0",
    "query-string": "^7.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-helmet": "^6.1.0",
    "react-hook-form": "^7.6.7",
    "react-intersection-observer": "^8.31.1",
    "react-responsive-carousel": "^3.2.21",
    "react-unity-webgl": "^7.1.10",
    "use-query-params": "^1.2.2",
    "xml-js": "^1.6.11"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.2",
    "@graphql-codegen/introspection": "^4.0.3",
    "@graphql-codegen/typescript-graphql-request": "^6.2.0",
    "@types/node": "^15.0.2",
    "@types/qrcode.react": "^1.0.1",
    "@types/react": "^17.0.5",
    "@types/react-dom": "^17.0.4",
    "@types/react-helmet": "^6.1.1",
    "@typescript-eslint/eslint-plugin": "^4.23.0",
    "@typescript-eslint/parser": "^4.23.0",
    "eslint": "^7.26.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^6.0.0",
    "lint-staged": "^11.0.0",
    "prettier": "2.3.0",
    "typescript": "^4.9.5"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "0BSD",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "lint": "eslint --fix \"src**/*.{js,jsx,ts,tsx}\"",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json}\"",
    "lint-test": "eslint \"src/**/*.{js,jsx,ts,tsx}\"",
    "format-test": "prettier --check \"src/**/*.{js,jsx,ts,tsx,json}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 0",
    "codegen": "graphql-codegen --config codegen.yml",
    "prepare": "husky install",
    "lint-staged": "lint-staged",
    "update-feed": "./update-feed.bash"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-default"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/*.{js,jsx,ts,tsx}": [
      "npm run lint-test"
    ],
    "src/*.{js,jsx,ts,tsx,json}": [
      "npm run format-test"
    ]
  }
}
